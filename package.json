{
    "name": "interface-forge",
    "author": "Na'aman Hirschfeld",
    "version": "1.2.2",
    "license": "MIT",
    "description": "Gracefully generate testing data using TypeScript",
    "keywords": [
        "typescript",
        "factory",
        "mock",
        "interface",
        "testing"
    ],
    "repository": "https://github.com/Goldziher/interfaceForge",
    "bugs": "https://github.com/Goldziher/interfaceForge/issues",
    "main": "dist/index.js",
    "typings": "dist/index",
    "files": [
        "dist/"
    ],
    "scripts": {
        "build": "rimraf dist && tsc -p ./tsconfig.build.json",
        "lint": "eslint --fix '**/*.ts'",
        "prepare": "husky install",
        "test": "jest",
        "test:coverage": "jest --coverage",
        "test:watch": "jest --watch"
    },
    "devDependencies": {
        "@sprylab/eslint-config": "^1.5.0",
        "@types/jest": "^26.0.24",
        "@types/node": "^16.3.1",
        "eslint": "^7.30.0",
        "husky": ">=7",
        "jest": "^27.0.6",
        "lint-staged": ">=11",
        "prettier": "^2.3.2",
        "rimraf": "^3.0.2",
        "ts-jest": "^27.0.3",
        "ts-node": "^10.1.0",
        "typescript": "^4.3.5"
    },
    "lint-staged": {
        "*.ts": "eslint --fix",
        "*.{js,md,json,yaml,yml}": "prettier --write"
    }
}
