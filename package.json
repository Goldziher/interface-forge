{
    "name": "interface-forge",
    "author": "Na'aman Hirschfeld",
    "contributors": [
        {
            "name": "Na'aman Hirschfeld",
            "email": "nhirschfeld@gmail.com"
        },
        {
            "name": "Damian Kress",
            "email": "mail@damiankress.de"
        }
    ],
    "version": "1.4.0",
    "license": "MIT",
    "description": "Gracefully generate testing data using TypeScript",
    "keywords": [
        "factory",
        "fixture",
        "interface",
        "jest",
        "mocha",
        "mock data",
        "mock",
        "test data",
        "testing",
        "typescript"
    ],
    "repository": "https://github.com/Goldziher/interfaceForge",
    "bugs": "https://github.com/Goldziher/interfaceForge/issues",
    "main": "dist/index.js",
    "typings": "dist/index",
    "files": [
        "dist/"
    ],
    "scripts": {
        "build": "rimraf dist && tsc -p ./tsconfig.build.json",
        "contributors:add": "all-contributors add",
        "contributors:generate": "all-contributors generate",
        "format": "prettier --write '**/*.{js,md,json,yaml,yml}'",
        "lint": "eslint --fix '**/*.ts'",
        "prepare": "husky install",
        "test": "jest",
        "test:coverage": "jest --coverage",
        "test:watch": "jest --watch"
    },
    "devDependencies": {
        "@sprylab/eslint-config": "^1.5.3",
        "@types/jest": "^26.0.24",
        "@types/node": "^16.4.0",
        "all-contributors-cli": "^6.20.0",
        "eslint": "^7.31.0",
        "husky": ">=7",
        "jest": "^27.0.6",
        "lint-staged": ">=11",
        "prettier": "^2.3.2",
        "rimraf": "^3.0.2",
        "ts-jest": "^27.0.4",
        "ts-node": "^10.1.0",
        "typescript": "^4.3.5"
    },
    "lint-staged": {
        "*.ts": "eslint --fix",
        "*.{js,md,json,yaml,yml}": "prettier --write"
    }
}
