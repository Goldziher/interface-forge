{
    "name": "interface-forge",
    "author": "Na'aman Hirschfeld",
    "version": "1.0.1",
    "license": "MIT",
    "description": "Gracefully generate testing data using TypeScript",
    "keywords": [
        "typescript",
        "factory",
        "mock",
        "interface",
        "testing"
    ],
    "repository": "https://github.com/Goldziher/interfaceForge",
    "bugs": "https://github.com/Goldziher/interfaceForge/issues",
    "main": "dist/index.js",
    "typings": "dist/index",
    "files": [
        "dist/"
    ],
    "scripts": {
        "build": "rimraf dist && tsc -p ./tsconfig.build.json",
        "lint": "eslint --fix 'src/**/*.ts'",
        "prepare": "husky install",
        "test": "jest",
        "test:coverage": "jest --coverage",
        "test:watch": "jest --watch"
    },
    "devDependencies": {
        "@types/jest": "^26.0.23",
        "@typescript-eslint/eslint-plugin": "^4.23.0",
        "@typescript-eslint/parser": "^4.23.0",
        "eslint": "^7.26.0",
        "eslint-config-prettier": "^8.3.0",
        "husky": ">=6",
        "jest": "^26.6.3",
        "lint-staged": ">=11",
        "prettier": "^2.3.0",
        "rimraf": "^3.0.2",
        "ts-jest": "^26.5.6",
        "ts-node": "^9.1.1",
        "typescript": "^4.2.4"
    },
    "lint-staged": {
        "*.ts": "eslint --fix",
        "*.{js,md,json,yaml,yml}": "prettier --write"
    }
}
