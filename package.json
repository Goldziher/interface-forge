{
    "name": "interface-forge",
    "author": "Na'aman Hirschfeld",
    "maintainers": [
        {
            "name": "Na'aman Hirschfeld",
            "email": "nhirschfeld@gmail.com"
        }
    ],
    "version": "1.6.1",
    "license": "MIT",
    "description": "Gracefully generate testing data using TypeScript",
    "keywords": [
        "factory",
        "fixture",
        "testing",
        "typescript"
    ],
    "repository": "https://github.com/tool-belt/interface-forge",
    "bugs": "https://github.com/tool-belt/interface-forge/issues",
    "main": "dist/index.js",
    "typings": "dist/index",
    "files": [
        "dist/"
    ],
    "scripts": {
        "preinstall": "npx only-allow pnpm",
        "clean": "rimraf dist",
        "clean:nodemodules": "rimraf node_modules",
        "build": "rimraf dist && tsc -p ./tsconfig.build.json",
        "contributors:add": "all-contributors add",
        "contributors:generate": "all-contributors generate",
        "format": "prettier --write '**/*.{js,md,json,yaml,yml}'",
        "lint": "eslint --fix '**/*.ts'",
        "prepare": "husky install",
        "test": "vitest run",
        "test:coverage": "vitest run --coverage",
        "test:watch": "vitest"
    },
    "devDependencies": {
        "@tool-belt/eslint-config": "^3.0.5",
        "@types/node": "^20.6.1",
        "@vitest/coverage-v8": "^0.34.4",
        "eslint": "^8.49.0",
        "husky": "^8.0.3",
        "lint-staged": "^14.0.1",
        "prettier": "^3.0.3",
        "rimraf": "^5.0.1",
        "ts-node": "^10.9.1",
        "typescript": "^5.2.2",
        "vitest": "^0.34.4"
    },
    "lint-staged": {
        "*.ts": "eslint --fix",
        "*.{js,md,json,yaml,yml}": "prettier --write"
    },
    "dependencies": {
        "@tool-belt/type-predicates": "^1.2.2"
    }
}
