{
    "$schema": "https://typedoc.org/schema.json",
    "entryPoints": ["./src/index.ts"],
    "out": "./docs",
    "readme": "README.md",
    "tsconfig": "./tsconfig.json",
    "theme": "github",
    "plugin": ["typedoc-plugin-missing-exports", "typedoc-plugin-coverage"],
    "includeVersion": true,
    "excludePrivate": true,
    "excludeInternal": true,
    "excludeExternals": false,
    "categorizeByGroup": true,
    "navigation": {
        "includeCategories": true,
        "includeGroups": true
    },
    "hideGenerator": true,
    "githubPages": true,
    "searchInComments": true,
    "cleanOutputDir": true,
    "validation": {
        "notExported": true,
        "invalidLink": true,
        "notDocumented": false
    }
}
