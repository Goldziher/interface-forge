{
    "$schema": "https://typedoc.org/schema.json",
    "entryPoints": ["./src/index.ts", "./src/zod.ts", "./examples/*.ts"],
    "out": "./docs",
    "readme": "README.md",
    "tsconfig": "./tsconfig.json",
    "plugin": [
        "typedoc-plugin-missing-exports",
        "typedoc-plugin-coverage",
        "typedoc-github-theme"
    ],
    "includeVersion": true,
    "excludePrivate": true,
    "excludeInternal": true,
    "excludeExternals": false,
    "categorizeByGroup": true,
    "navigation": {
        "includeCategories": true,
        "includeGroups": true
    },
    "hideGenerator": true,
    "githubPages": true,
    "searchInComments": true,
    "cleanOutputDir": true,
    "validation": {
        "notExported": true,
        "invalidLink": true,
        "notDocumented": false
    }
}
